<?xml version="1.0" encoding="UTF-8"?>
<MoDeL>
	<dbinterface>
		<input>
			<listOfParameters>
				<parameter db="Temperatrue">
					<id>Temperature</id>
					<units>kelvin</units>
					<constant>false</constant>
				</parameter>
			</listOfParameters>
			<listOfRules>
				<assignmentrule>
					<variable>Temperature</variable>
					<math>exp(t/unit_t)*unit_temperature</math>
				</assignmentrule>
				<raterule>
					<variable>IPTG</variable>
					<math>sin(t/unit_t)*unit_concentration</math>
				</raterule>
				<raterule>
					<variable>E_coli</variable>
					<math>unit_volume * pow(t,2)</math>
				</raterule>
			</listOfRules>
			<listOfCompartments>
				<compartment db="Flask">
					<id>Flask</id> 
					<size>0.25</size>
					<outside>ROOT</outside>
				</compartment>
				<compartment db="E_coli">
					<id>E_coli</id>
					<outside>Flask</outside>
					<constant>false</constant>
				</compartment>
			</listOfCompartments>
			<listOfSpecies>
				<species db="E_coli_cell" ccid="E_coli">
					<id>E_coli_cell</id><!--sbml id-->
					<compartment>Flask</compartment>
					<initialConcentration>1.6E-14</initialConcentration>
					<constant>false</constant>
				</species>
				<species db="IPTG_molecule">
					<id>IPTG</id><!--sbml id-->
					<compartment>Flask</compartment>
					<constant>false</constant>
				</species>
				<species>
					<id>DNA1</id>
					<compartment>E_coli</compartment>
					<cnModel>
						<listOfChains>
							<chain>
								<listOfParts>
									<part>
										<partReference>pSB1A3</partReference>
										<partLabel>pSB1A3_1</partLabel>
										<partType>ForwardDNA</partType>
										<partCategory>plasmidBackbone</partCategory>
									</part>
									<part>
										<partReference>placI185</partReference>
										<partLabel>placI185_1</partLabel>
										<partType>ForwardDNA</partType>
										<partCategory>biobrick</partCategory>
									</part>
									<part>
										<partReference>cIlam156</partReference>
										<partLabel>cIlam156_1</partLabel>
										<partType>ForwardDNA</partType>
										<partCategory>biobrick</partCategory>
									</part>
								</listOfParts>
							</chain>
						</listOfChains>
					</cnModel>
				</species>
			</listOfSpecies>
		</input>
	</dbinterface>
</MoDeL>
