												<minus/>
												<ci> t  </ci>
												<ci> t0 </ci>
											</apply>
											<ci> sigma </ci>
										</apply>
									</apply>
									<cn type="integer"> 2 </cn>
								</apply>
							</apply>
						</apply>
					</lambda>
				</math>
			</functionDefinition>
		</listOfFunctionDefinitions>
